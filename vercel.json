 {
        "version": 2,
        "name": "Newsletter",
        "builds": [
          {
            "src": "app.js",
            "use": "@vercel/node"
          }, 
          {
            "source": "/signin.css",
            "destination": "/public/signin.css"
          }
        ],
        "routes": [
          {
            "src": "/(.*)",
            "dest": "app.js"
          }
        ]
      }
  