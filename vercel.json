{
    "version": 2,
    "name": "Newsletter",
    "builds": [
      {
        "src": "app.js",
        "use": "@vercel/node"
      },
      {
        "src": "signin.css",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/app.js",
        "dest": "/app.js"
      },
      {
        "src": "/signin.css",
        "dest": "/signin.css"
      },
      {
        "src": "/(.*)",
        "dest": "/app.js"
      }
    ]
  }
  